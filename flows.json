    {
        "id": "ca893050658f118b",
        "type": "function",
        "z": "df9f7062531e91f4",
        "name": "Format Weather",
        "func": "// msg.weather.timezone = `Timezone is GMT ${msg.payload.timezone / 3600}:00 `\nmsg.payload.weather_new = {}\n// let timezone1 = msg.payload.timezone\nmsg.payload.weather_new.timezone = `GMT ${msg.payload.timezone / 3600}:00 `\nmsg.payload.weather_new.description = `${msg.payload.weather[0].description}`\nmsg.payload.weather_new.temp = `${msg.payload.main.temp} Â°C`\nmsg.payload.weather_new.feels_like = `${msg.payload.main.feels_like} Â°C` \nmsg.payload.weather_new.humidity = `${msg.payload.main.humidity} %`\nlet wind_speed = msg.payload.wind.speed * 3.6\nlet wind_speed_str = wind_speed.toString()\nlet wind_speed_final = wind_speed_str.substr(0, 5)\n\nmsg.payload.weather_new.wind_speed = `${wind_speed_final} km/hr`\n\nlet sunrise_time = msg.payload.sys.sunrise\nlet sunset_time = msg.payload.sys.sunset\n// Create a new JavaScript Date object based on the timestamp\n// multiplied by 1000 so that the argument is in milliseconds, not seconds.\nvar date_for_sunrise = new Date(sunrise_time * 1000);\nvar date_for_sunset = new Date(sunset_time * 1000);\n// Hours part from the timestamp\nvar hours_for_sunrise = date_for_sunrise.getHours();\nvar hours_for_sunset = date_for_sunset.getHours();\n// Minutes part from the timestamp\nvar minutes_sunrise = \"0\" + date_for_sunrise.getMinutes();\nvar minutes_sunset = \"0\" + date_for_sunset.getMinutes();\n// Will display time in 10:30:23 format\nvar formattedTime_sunrise = hours_for_sunrise + ':' + minutes_sunrise.substr(-2) + \" KST\"\nvar formattedTime_sunset = hours_for_sunset + ':' + minutes_sunset.substr(-2) + \" KST\"\n\nmsg.payload.weather_new.sunrise = formattedTime_sunrise\nmsg.payload.weather_new.sunset = formattedTime_sunset\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
